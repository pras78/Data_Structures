package Sample;

public class Swapping {

	static void swappingWithoutTempVariable(int a, int b) {

		a = a + b;
		b = a - b;
		a = a - b;
		System.out.println("Reversed values for a and b are: " + a + " & " + b);

	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		swappingWithoutTempVariable(12, 11);

	}

}
